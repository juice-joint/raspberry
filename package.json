{
  "name": "raspberry",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "clean": "turbo clean && rm -rf node_modules",
    "tauri:dev": "turbo run tauri:dev --filter=goldie",
    "tauri:build": "turbo run tauri:build --filter=goldie",
    "build:tauri": "turbo run tauri:build --filter=goldie"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "turbo": "^2.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "typescript": "~5.6.2"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "bun@1.2.3"
}